<!DOCTYPE utfx:tests PUBLIC "-//UTF-X//DTD utfx-tests 1.0//EN" "utfx_tests.dtd">
<utfx:tests xmlns:utfx="http://utfx.org/test-definition">
	<utfx:stylesheet src="import_precedence_tests.xsl"/>

	<!-- tests whether the root matcher of UTF-X wrapper doc matches before
	     the root matcher and utfx-wrapper in the stylesheet under test 
	     -->
	<utfx:test>
		<utfx:name>root matcher test</utfx:name>
		<utfx:assert-equal>
			<utfx:source>
				<tree/>
			</utfx:source>
			<utfx:expected>
				<el name="tree"/>
			</utfx:expected>
		</utfx:assert-equal>
	</utfx:test>

</utfx:tests>
